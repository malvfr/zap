{
  "name": "zap-cli",
  "version": "1.0.0",
  "description": "Database data generation tool",
  "main": "index.js",
  "bin": {
    "zap": "build/cli/cli.js"
  },
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "start:dev": "nodemon",
    "start:prod": "node build/index.js",
    "start:debug": "nodemon --debug --watch",
    "build": "rimraf ./build && tsc && npm link",
    "start": "npm run build && node build/index.js",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malvfr/zap.git"
  },
  "keywords": [
    "database",
    "mock",
    "generation",
    "sql",
    "cli",
    "node",
    "javascript"
  ],
  "author": "Matheus Alvarenga",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malvfr/zap/issues"
  },
  "homepage": "https://github.com/malvfr/zap#readme",
  "devDependencies": {
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "commander": "^6.2.1",
    "js-yaml": "^4.0.0",
    "faker": "^5.1.0"
  }
}
